{
  "name": "node-ts-boilerplate",
  "version": "0.0.0",
  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "main": "src/main.js",
  "homepage": "https://github.com/jiifw/node-ts-boilerplate",
  "scripts": {
    "start": "node build/main.js",
    "dev": "yarn run build:watch",
    "clean": "rimraf coverage build tmp runtime && mkdir coverage runtime build",
    "build": "yarn run clean && babel src --out-dir ./build --extensions .ts,.tsx,.js,.jsx",
    "build:watch": "nodemon --exec babel-node ./src/main.ts --extensions .ts,.tsx,.js,.jsx",
    "build:debug": "nodemon --inspect --exec babel-node ./src/main.ts --extensions .ts,.tsx,.js,.jsx",
    "build:prod": "yarn run clean && babel framework/ src/ --out-dir ./build --extensions .ts,.tsx,.js,.jsx  --copy-files --minified --no-comments --source-maps",
    "prebuild": "npm run lint",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc --write .",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "sequelize:migrate": "node framework/db/sequelize/cli/migrate",
    "sequelize:seed": "node framework/db/sequelize/cli/seed",
    "cli": "ts-node -r tsconfig-paths/register framework/cli.ts"
  },
  "author": {
    "name": "Junaid Atari",
    "email": "mj.atari@gmail.com",
    "url": "https://github.com/blacksmoke26"
  },
  "engines": {
    "node": ">= 18.15"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@fastify/accepts": "^4.3.0",
    "@fastify/cookie": "^9.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/middie": "^8.3.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/session": "^10.7.0",
    "@fastify/swagger": "^8.12.1",
    "@fastify/url-data": "^5.4.0",
    "@inquirer/prompts": "^3.3.0",
    "@types/nunjucks": "^3.2.6",
    "bcryptjs": "^2.4.3",
    "case": "^1.6.3",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.1",
    "fastify-favicon": "^4.3.0",
    "fastify-graceful-shutdown": "^3.5.1",
    "figlet": "^1.7.0",
    "glob": "^10.3.10",
    "http-errors": "^2.0.0",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "ms": "^2.1.3",
    "nanoid": "^5.0.4",
    "nunjucks": "^3.2.4",
    "object-path": "^0.11.8",
    "pagination": "^0.4.6",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.35.2",
    "sequelize-cursor-pagination": "^3.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "umzug": "^3.5.0",
    "x-xss-protection": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/node": "^7.22.19",
